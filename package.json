{
  "name": "generator-jhipster-jooq",
  "version": "1.0.0",
  "description": "jOOQ blueprint for JHipster",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-7"
  ],
  "homepage": "https://github.com/jhipster/generator-jhipster-jooq",
  "bugs": {
    "url": "https://github.com/jhipster/generator-jhipster-jooq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhipster/generator-jhipster-jooq.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/generator-jhipster"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "JHipster",
    "url": "https://github.com/jhipster"
  },
  "type": "module",
  "imports": {
    "#test-utils": "./test/utils.mjs"
  },
  "files": [
    "generators"
  ],
  "scripts": {
    "ejslint": "ejslint generators/**/*.ejs && ejslint generators/**/*.ejs -d '&'",
    "lint": "eslint .",
    "lint-fix": "npm run ejslint && npm run lint -- --fix",
    "mocha": "mocha generators --no-insight --forbid-only",
    "prettier-check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,cjs,mjs,ts,tsx,css,scss,vue,java}\"",
    "prettier-format": "prettier --write \"{,**/}*.{md,json,yml,html,js,cjs,mjs,ts,tsx,css,scss,vue,java}\"",
    "pretest": "npm run prettier-check && npm run lint",
    "test": "npm run mocha --",
    "update-snapshot": "npm run mocha -- --no-parallel --updateSnapshot"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "generator-jhipster": "github:mshima/generator-jhipster#skip_ci-generate_blueprint"
  },
  "devDependencies": {
    "ejs-lint": "1.2.2",
    "eslint": "8.23.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect": "28.1.3",
    "mocha": "9.2.2",
    "mocha-expect-snapshot": "4.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-java": "1.6.2",
    "prettier-plugin-packagejson": "2.2.18",
    "yeoman-test": "6.3.0"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
